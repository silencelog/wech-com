{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": 'v12.16.2'
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-async-to-generator"
  ],
  "ignore": [
    "dist",
    ".git",
    "node_modules",
    ".vscode",
    "logs",
    "package-lock.json",
    "package.json",
    "app-babel.js",
    ".gitignore",
    "test"
  ]
}
